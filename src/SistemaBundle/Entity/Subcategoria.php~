<?php

namespace SistemaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 *  Description of Produtos
 *
 * @author Criaative
 * 
 * @ORM\Entity
 * @ORM\Table(name="categorias")
 */
class Subcategoria 
{
    /**
     * @ORM\Column(type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;
    
    /**
     * @ORM\Column(type="string", length=100)
     */
    private $nome;
}



